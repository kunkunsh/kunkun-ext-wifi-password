{
  "$schema": "https://schema.kunkun.sh",
  "name": "kunkun-ext-wifi-password",
  "version": "0.0.2",
  "type": "module",
  "kunkun": {
    "name": "Wifi Password",
    "shortDescription": "Show Wifi Password",
    "longDescription": "Show Wifi Password",
    "identifier": "wifi-password",
    "permissions": [
      "clipboard:write-text",
      {
        "permission": "shell:execute",
        "allow": [
          {
            "cmd": {
              "program": "networksetup",
              "args": [
                "-listpreferredwirelessnetworks",
                "en0"
              ]
            }
          },
          {
            "cmd": {
              "program": "security",
              "args": [
                "find-generic-password",
                "-D",
                "AirPort network password",
                "-a",
                ".+",
                "-w"
              ]
            }
          }
        ]
      }
    ],
    "demoImages": [],
    "icon": {
      "type": "iconify",
      "value": "material-symbols:extension"
    },
    "customUiCmds": [],
    "templateUiCmds": [
      {
        "name": "Show Wifi Password",
        "main": "dist/index.js",
        "cmds": []
      }
    ]
  },
  "scripts": {
    "dev": "bun build.ts dev",
    "build": "bun build.ts"
  },
  "dependencies": {
    "@kksh/api": "^0.0.52",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/qrcode": "^1.5.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "./dist",
    ".gitignore"
  ],
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a",
  "module": "index.ts"
}